{
  "name": "cambot-mineflayer-prismarine-viewer",
  "version": "1.0.0",
  "description": "A Mineflayer bot for cinematic camera work in Minecraft.",
  "main": "cameraBot.js",
  "engines": {
    "node": ">=22"
  },
  "scripts": {
    "start": "node cameraBot.js",
    "setup:macos": "bash scripts/setup-macos.sh",
    "verify": "bash scripts/verify.sh",
    "update": "git pull --ff-only && npm ci && npm run verify",
    "test": "node runTests.js",
    "analyze:logs": "node scripts/analyze-logs.js --last",
    "auth:prompt": "node scripts/auth-prompt.js",
    "auth:reset": "node scripts/auth-reset.js"
  },
  "dependencies": {
    "keytar": "^7.9.0",
    "mineflayer": "^4.20.1",
    "mineflayer-pathfinder": "^2.4.4"
  },
  "overrides": {
    "axios": "^1.6.8",
    "@xboxreplay/xboxlive-auth": "^5.0.2",
    "prismarine-auth": {
      "@xboxreplay/xboxlive-auth": "^5.0.2",
      "axios": "^1.6.8"
    },
    "@xboxreplay/xboxlive-auth@*": {
      "axios": "^1.6.8"
    },
    "minecraft-data": "^3.97.0",
    "minecraft-protocol": {
      "minecraft-data": "^3.97.0"
    }
  }
}